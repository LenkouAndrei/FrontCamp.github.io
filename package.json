{
  "name": "FrontCamp.github.io",
  "version": "1.0.0",
  "description": "FrontCamp program 2018",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "build": "babel src -d dist --plugins @babel/plugin-proposal-object-rest-spread",
    "watch": "babel dist --watch --out-file index.js"
  },
  "lint-staged": {
    "src/*.js": [
      "prettier --single-quote --trailing-comma es5 --config-precedence=prefer-file --write \"src/*.js\"",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LenkouAndrei/FrontCamp.github.io.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LenkouAndrei/FrontCamp.github.io/issues"
  },
  "homepage": "https://github.com/LenkouAndrei/FrontCamp.github.io#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-env": "^7.1.6",
    "babel-loader": "8.0.4",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "husky": "1.1.4",
    "lint-staged": "8.0.5",
    "prettier": "1.15.2",
    "whatwg-fetch": "^3.0.0"
  },
  "dependencies": {}
}
